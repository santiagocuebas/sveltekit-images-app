		"@typescript-eslint/eslint-plugin": "5.56.0",
		"@typescript-eslint/parser": "5.56.0",
		"eslint": "8.36.0",
		"eslint-plugin-svelte3": "4.0.0",
		"svelte": "3.57.0",
		"svelte-check": "3.1.4",
		"svelte-preprocess": "5.0.3",
		"tslib": "2.5.0",
		"typescript": "5.0.2",
		"vite": "4.2.1"
	},
	"dependencies": {
		"dotenv": "16.0.3"
